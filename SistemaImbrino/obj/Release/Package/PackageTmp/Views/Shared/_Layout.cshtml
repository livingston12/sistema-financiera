        // Llenar drowdown en clientes pantalla cuotas vencidas
        function llenarDrop(data, Report) {

			var IdDw = "-- Selecciona un cliente --";
			var texto = "";
			if (Report == "CV") {
				IdDw = "#dwClientes";
			}
			else if (Report == "FA") {
				IdDw = "#dwClientesFA";
			}
			else if (Report == "IC") {
				IdDw = "#dwClientesIC";
			}
			else if (Report == "FinIC") {
				IdDw = "#dwFinIC";
				texto = "-- Selecciona un financiamiento --";
			}
			else if (Report == "CB") {
				IdDw = "#dwCuentaCB";
				texto = "-- Selecciona una cuenta --"
			}
			else if (Report == "CA") {
				IdDw = "#CAR_NUMFIN";
				texto = "-- Seleccione un financiamineto --"
			}

            $(IdDw).append($('<option />', {
				text: texto,
                value: '',
            }));

            $(data).each(function (index, element) {
                $(IdDw).append($('<option />', {
                    text: element.value,
                    value: element.id,
                }));


			});
            $(IdDw).selectMania('update');

        }

