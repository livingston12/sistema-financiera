@model SistemaImbrino.Models.CARGO



<tr>
	@{
		string nombre_completo = Model.CAR_DESCRI;
		string isEnable = Model.enable == 1 ? "SI" : "NO";
	}
	<td>
		@nombre_completo
	</td>
	<td>
		@isEnable
	</td>



	<td class="col-lg-1 text-center">
		<span class="glyphicon glyphicon-edit " onclick="editar_Popup(@Model.CAR_CODIGO.ToString())" id="Ceditar" aria-hidden="true"></span>
		<span class="glyphicon glyphicon-trash control-label" onclick="EliminarCurrentCargo(@Model.CAR_CODIGO,'@nombre_completo')" id="Celiminar" aria-hidden="true"></span>

	</td>

</tr>