@model SistemaImbrino.Models.VENDEDOR


<input type="hidden" id="VEN_CODIGO" name="VEN_CODIGO"/>
<div class="row">
	<div class="col-xs-12 col-sm-6 col-lg-6 ">
		<label class="control-label" for="EVEN_NOMBRE">Nombre</label>
		<div class="form-group" id="dvEVEN_NOMBRE">
			<div class="input-group">
				<label class="input-group-addon" for="EVEN_NOMBRE"><i class="glyphicon glyphicon-user"></i></label>
				@Html.EditorFor(model => model.VEN_NOMBRE, new { htmlAttributes = new { @class = "form-control texto", @id = "EVEN_NOMBRE" } })
				<span id="spanEVEN_NOMBRE" class="glyphicon  form-control-feedback" aria-hidden="true"></span>
			</div>
		</div>
	</div>
</div>

