				}
				else {
					dwFinIC.removeAttr("disabled", "disabled");
					   var url = "@Url.Action("GetFIns", "IngresoCuotas")";
				$.ajax({
					type: "GET",
					url: url,
					dataType: "json",
					data: {
						clienteID: idCLiente
					},
					success: function (_data) {
						llenarDrop(_data, 'FinIC');
					}
				});
					dwFinIC.selectMania('update');
