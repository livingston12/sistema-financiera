@model SistemaImbrino.Models.OTROSCR



<head>
	<style>
		.datepickerFormat {
			z-index: 1500 !important;
			position: absolute !important;
		}
	</style>
</head>
<div class="row">
	<div class="col-xs-12 col-sm-6 col-lg-4 ">
		<label class="control-label" for="TIPO_CREDITO">Tipo credito</label>
		<div class="form-group" id="dvTIPO_CREDITO">
			<div class="input-group">
				<label class="input-group-addon" for="TIPO_CREDITO"><i class="glyphicon glyphicon-credit-card"></i></label>
				<select id="TIPO_CREDITO" class="form-control select"></select>
				<span id="spanTIPO_CREDITO" class="glyphicon  form-control-feedback" aria-hidden="true"></span>
			</div>
		</div>
	</div>
	<div class="col-xs-12 col-sm-6 col-lg-4 ">
		<label class="control-label" for="BANCO">Banco</label>
		<div class="form-group" id="dvBANCO">
			<div class="input-group">
				<label class="input-group-addon" for="BANCO"><i class="glyphicon glyphicon-home"></i></label>
				<select id="BANCO" class="form-control select widtTotal"></select>
				<span id="spanBANCO" class="glyphicon  form-control-feedback" aria-hidden="true"></span>
			</div>
		</div>
	</div>

	<div class="col-xs-12 col-sm-6 col-lg-4 ">
		<label class="control-label" for="CUENTA_BANCARIA">Cuenta bancaria</label>
		<div class="form-group" id="dvCUENTA_BANCARIA">
			<div class="input-group">
				<label class="input-group-addon" for="CUENTA_BANCARIA"><i class="glyphicon glyphicon-th-list"></i></label>
				<select id="CUENTA_BANCARIA" class="form-control select widtTotal"></select>
				<span id="spanCUENTA_BANCARIA" class="glyphicon  form-control-feedback" aria-hidden="true"></span>
			</div>
		</div>
	</div>


</div>


<div class="row">
	<div class="col-xs-12 col-sm-6 col-lg-4 ">
		<label class="control-label" id="lbl_NUMERO_CHEQUE" for="NUMERO_CHEQUE">Numero cheque</label>
		<div class="form-group" id="dvNUMERO_CHEQUE">
			<div class="input-group">
				<label class="input-group-addon" for="NUMERO_CHEQUE"><i class="glyphicon glyphicon-folder-open"></i></label>
				@Html.EditorFor(model => model.NUMERO_CHEQUE, new { htmlAttributes = new { @class = "form-control texto" } })
				<span id="spanNUMERO_CHEQUE" class="glyphicon  form-control-feedback" aria-hidden="true"></span>
			</div>
		</div>
	</div>

	<div class="col-xs-12 col-sm-6 col-lg-4 ">
		<label class="control-label" for="TIPO_SALIDA">Tipo salida</label>
		<div class="form-group" id="dvTIPO_SALIDA">
			<div class="input-group">
				<label class="input-group-addon" for="TIPO_SALIDA"><i class="glyphicon glyphicon-share-alt"></i></label>
				<select id="TIPO_SALIDA" class="form-control select widtTotal"></select>
				<span id="spanTIPO_SALIDA" class="glyphicon  form-control-feedback" aria-hidden="true"></span>
			</div>
		</div>
	</div>

	<div class="col-xs-12 col-sm-6 col-lg-4 ">
		<label class="control-label" for="MONTO">Monto</label>
		<div class="form-group" id="dvMONTO">
			<div class="input-group">
				<label class="input-group-addon" for="MONTO"><i class="glyphicon glyphicon-usd"></i></label>
				@Html.EditorFor(model => model.MONTO, new { htmlAttributes = new { @class = "form-control number numberDecimal" } })
				<span id="spanMONTO" class="glyphicon  form-control-feedback" aria-hidden="true"></span>
			</div>
		</div>
	</div>
</div>

<div class="row">
	<div class="col-xs-12 col-sm-6 col-lg-4 ">
		<label class="control-label" for="FECHA">Fecha</label>
		<div class="form-group" id="dvFECHA">
			<div class="input-group">
				<label class="input-group-addon" for="FECHA"><i class="glyphicon glyphicon-calendar"></i></label>
				<input type="text" class="form-control date datepicker" placeholder="Fecha" id="FECHA">
				<span id="spanFECHA" class="glyphicon  form-control-feedback" aria-hidden="true"></span>
			</div>
		</div>
	</div>

	<div class="col-xs-12 col-sm-6 col-lg-4 ">
		<label class="control-label" id="lbl_BENEFICIARIO" for="BENEFICIARIO">Beneficiario</label>
		<div class="form-group" id="dvBENEFICIARIO">
			<div class="input-group">
				<label class="input-group-addon" for="BENEFICIARIO"><i class="glyphicon glyphicon-user"></i></label>
				<input type="text" class="form-control texto" id="BENEFICIARIO">
				<span id="spanBENEFICIARIO" class="glyphicon  form-control-feedback" aria-hidden="true"></span>
			</div>
		</div>
	</div>

	<div class="col-xs-12 col-sm-6 col-lg-4 ">
		<label class="control-label" for="CONCEPTO">Concepto</label>
		<div class="form-group" id="dvCONCEPTO">
			<div class="input-group">
				<label class="input-group-addon" for="CONCEPTO"><i class="glyphicon glyphicon-pencil"></i></label>
				<textarea rows="3" id="CONCEPTO" class="form-control texto"></textarea>
				<span id="spanCONCEPTO" class="glyphicon  form-control-feedback" aria-hidden="true"></span>
			</div>
		</div>
	</div>
</div>