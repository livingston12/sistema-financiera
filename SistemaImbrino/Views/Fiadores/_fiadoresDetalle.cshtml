@model SistemaImbrino.Models.FIADOR



<tr>
	@{ 
		string nombre_completo = Model.FIA_NOMBRE;
	}
	<td class="">
		@nombre_completo
	</td>
	<td>@Model.FIA_CEDULA</td>
	<td>@Model.FIA_TELEFO</td>
	<td class="">@Model.FIA_DIRECC</td>
	
	
	<td class="col-lg-1 text-center">
		<span class="glyphicon glyphicon-edit " onclick="editar_Popup(@Model.FIA_CODIGO)" id="Ceditar" aria-hidden="true"></span>
		<span class="glyphicon glyphicon-trash control-label" onclick="EliminarCurrentFiador(@Model.FIA_CODIGO,'@nombre_completo')" id="Celiminar" aria-hidden="true"></span>

	</td>   	  

</tr>