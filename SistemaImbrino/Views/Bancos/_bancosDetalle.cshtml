@model SistemaImbrino.Models.BANCO



<tr>
	@{
		string nombre_completo = Model.BCO_NOMBRE;
	}
	<td class="">
		@nombre_completo
	</td>
	<td>@Model.BCO_ABREVI</td>
	<td>@Model.BCO_SUCURS</td>
	<td class="">@Model.BCO_TELEF1</td>
	<td class="">@Model.BCO_TELEF2</td>
	<td class="">@Model.BCO_TELEF3</td>


	<td class="col-lg-1 text-center">
		<span class="glyphicon glyphicon-edit " onclick="editar_Popup(@Model.BCO_CODIGO)" id="Ceditar" aria-hidden="true"></span>
		<span class="glyphicon glyphicon-trash control-label" onclick="EliminarCurrentBanco(@Model.BCO_CODIGO,'@nombre_completo')" id="Celiminar" aria-hidden="true"></span>

	</td>

</tr>