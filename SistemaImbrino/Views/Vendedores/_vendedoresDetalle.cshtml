@model SistemaImbrino.Models.VENDEDOR



<tr>
	@{
		string nombre_completo = Model.VEN_NOMBRE;
	}
	<td class="">
		@nombre_completo
	</td>
	


	<td class="col-lg-1 text-center">
		<span class="glyphicon glyphicon-edit " onclick="editar_Popup(@Model.VEN_CODIGO)" id="Ceditar" aria-hidden="true"></span>
		<span class="glyphicon glyphicon-trash control-label" onclick="EliminarCurrentVendedor(@Model.VEN_CODIGO,'@nombre_completo')" id="Celiminar" aria-hidden="true"></span>

	</td>

</tr>